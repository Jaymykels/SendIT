{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "mocha": true,
        "jasmine": true
    },
    "rules": {
        "comma-dangle": "off",
        "no-unused-vars":[
            "error", {
                "vars": "local",
                "args": "none"
            }
        ],
        "linebreak-style": "off"
    }
}